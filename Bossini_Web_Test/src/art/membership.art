<div class="container">
    <ul class="items">
    {{each items}}
      <li class= "item">
        <div class= "title">
          <h3>{{$value.title}}</h3>
        </div>
        <div class="content">
          <p class="body2">{{#$value.content}}</p>
        </div>
      </li>
    {{/each}}
    </ul>
</div>
